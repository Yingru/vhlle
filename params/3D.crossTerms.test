outputDir       freezeout/IC11-12fm.eta0d1-zeta0d1.hotQCD
icInputFile     IC_PbPb2760_11-12fm.dat
eosFile         eos/eos_hotQCD.dat

eosType        3        ! EoS choice: 0 for Laine, 1 for Chiral, 2 for Bag, 3 for hotQCD
e_crit       0.3        !  criterion for surface finding

etaS         0.1        !  constant etaS
etaS_slope   0.0        ! eta/s slope above Tc
zetaS        0.1        !  zeta/s, bulk viscosity normalization
 

nx            81        ! number of cells in X direction
ny            81        ! number of cells in Y direction
nz            41        ! number of cells in eta direction
xmin       -16.0        ! coordinate of the first cell
xmax        16.0        ! coordinate of the last cell
ymin       -16.0
ymax        16.0
etamin      -8.0
etamax       8.0
tau0         0.6        ! starting proper time
tauMax      30.0        ! proper time to stop hydro
dtau         0.1        ! timestep


icModel        5        !1=Glauber; 2=Glauber+parametrized rapidity; 4=Gubser; 5=3D Trento
glauberVar     1        ! reserved for future use
s0ScaleFactor  1        !53.55
epsilon0    30.0
impactPar    7.0
ic_nxy       261        ! IC grid size
ic_neta      101     
ic_dxy       0.1        ! IC grid step
ic_deta      0.2

