outputDir       hydro_output/3D.etas01.zetas01.crossTerms

eosType       3           ! EoS choice: 0 for Laine, 1 for Chiral model, 2 for Bag model, 3 for hotQCD model

etaS      0.1             !  eta/s value
zetaS     0.1             !  zeta/s, bulk viscosity
e_crit    0.5             !  criterion for surface finding

nx        81             ! number of cells in X direction
ny        81             ! number of cells in Y direction
nz        51             ! number of cells in eta direction
xmin     -16.0            ! coordinate of the first cell
xmax      16.0            ! coordinate of the last cell
ymin     -16.0
ymax      16.0
etamin   -10.0
etamax    10.0

! icModel: 1=Glauber, 2=Glauber_table+parametrized rapidity for energy conserv. check, 4=Gubser flow; 5=3D trento (or any other 3D IC density)
icModel    5
glauberVar 1       ! reserved for future use
icInputFile 0.dat
eosFile eos/eos_hotQCD.dat 

s0ScaleFactor  1  !53.55
epsilon0   30.0
impactPar  7.0
tau0       0.6     ! starting proper time
tauMax     30.0    ! proper time to stop hydro
dtau       0.1    ! timestep
ic_nxy     161     ! IC grid size
ic_neta    21    
ic_dxy     0.2     !IC grid step
ic_deta    1 
